{
  "connection": "default",
  "collectionName": "Payment",
  "info": {
    "name": "payment",
    "description": "Aka invoice."
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "type": {
      "enum": [
        "credit_card",
        "check"
      ],
      "required": true,
      "type": "enumeration"
    },
    "account": {
      "enum": [
        "Foundation",
        "Association"
      ],
      "type": "enumeration"
    },
    "users": {
      "collection": "user",
      "via": "payments",
      "dominant": true,
      "plugin": "users-permissions"
    },
    "date": {
      "required": true,
      "type": "date"
    },
    "Status": {
      "enum": [
        "paid",
        "pending"
      ],
      "type": "enumeration"
    },
    "purchase": {
      "model": "purchase",
      "via": "payments"
    },
    "total": {
      "type": "decimal"
    },
    "products": {
      "collection": "product",
      "dominant": true,
      "via": "payments"
    }
  }
}