{
  "connection": "default",
  "collectionName": "Product",
  "info": {
    "name": "product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "type": {
      "required": true,
      "type": "string"
    },
    "discount": {
      "group": "discount",
      "repeatable": true,
      "type": "group"
    },
    "fee": {
      "group": "fee",
      "repeatable": true,
      "required": true,
      "type": "group"
    },
    "users": {
      "collection": "user",
      "via": "products",
      "dominant": true,
      "plugin": "users-permissions"
    },
    "purchases": {
      "collection": "purchase",
      "via": "product"
    },
    "payments": {
      "collection": "payment",
      "via": "products"
    }
  }
}